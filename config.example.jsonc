{
  "target": {
    "baseUrl": "https://ai-api.coding.global",
    "systemAccessToken": "your-system-access-token",
    "userId": 1
  },
  "preserveUnlistedProviders": true, // Keep channels from providers not in this config
  "blacklist": ["kiro", "nsfw"],
  "modelMapping": {
    "claude-sonnet-4-5-20250929-complex-name": "claude-sonnet-4-5"
  },
  "providers": [
    {
      "type": "newapi",
      "name": "newapi",
      "baseUrl": "https://www.newapi.ai",
      "systemAccessToken": "your-system-access-token",
      "userId": 1,
      "enabledVendors": ["anthropic", "openai"],
      "enabledModels": ["claude-*-4-5*", "gpt-5"],
      // Global: 67% cheaper than upstream (pay 33% of upstream price)
      "priceAdjustment": -0.67
      // Per-vendor (requires "default" key):
      // "priceAdjustment": {
      //   "default": -0.67,
      //   "anthropic": -0.50,
      //   "openai": -0.75,
      //   "google": 0.10
      // }
    },
    {
      "type": "direct",
      "name": "kimi",
      "vendor": "moonshot", // Maps to channel type 25 (MOONSHOT)
      "apiKey": "sk-kimi-your-api-key",
      // "baseUrl": "https://api.moonshot.cn", // Optional, defaults from vendor registry
      "enabledModels": ["kimi-*", "moonshot-*"], // Optional glob filters
      // "groupRatio": 1, // Optional, static ratio (default 1). Cannot use with priceAdjustment
      "priceAdjustment": -0.1 // Optional, adjusts ratio relative to cheapest upstream (-0.1 = 10% cheaper)
    },
    {
      "type": "sub2api",
      "name": "sub2api",
      "baseUrl": "https://demo.sub2api.org",
      "adminApiKey": "admin-your-admin-api-key", // If admin use this
      "groups": [
        { "key": "sk-...", "platform": "anthropic" },
        { "key": "sk-...", "platform": "openai" },
        { "key": "sk-...", "platform": "gemini" }
      ], // If not admin, use groups to specify API keys and platforms
      "enabledVendors": ["anthropic", "openai", "google"],
      "priceAdjustment": -0.1 // Also supports per-vendor object format
    }
  ]
}
